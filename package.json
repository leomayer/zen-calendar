{
  "name": "zen-calendar",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4741",
    "build": "node src/app/build.js",
    "watch": "ng build --watch --configuration development",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier-watch": "onchange \"./{src,e2e}/**/*.{ts,js,html,scss}\" -- prettier --write  --ignore-unknown {{changed}}",
    "test": "ng test",
    "start-zenCal": "npm run start & npm run prettier-watch",
    "github-pages": "ng deploy --base-href=/zen-calendar/",
    "sme": "ng build --configuration production --source-map=true --named-chunks==true && source-map-explorer dist/zen-calendar/**/*.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
        "@angular/cdk": "^18.0.0",
        "@angular/common": "^18.0.0",
        "@angular/compiler": "^18.0.0",
        "@angular/core": "^18.0.0",
        "@angular/elements": "^18.0.0",
        "@angular/forms": "^18.0.0",
        "@angular/material": "^18.0.0",
        "@angular/material-luxon-adapter": "^18.2.10",
        "@angular/platform-browser": "^18.0.0",
        "@angular/platform-browser-dynamic": "^18.2.9",
        "@angular/router": "^18.2.9",
        "@ngrx/signals": "^18.0.2",
        "@fullcalendar/angular": "^6.1.10",
        "@fullcalendar/core": "^6.1.11",
        "@fullcalendar/daygrid": "^6.1.11",
        "@fullcalendar/interaction": "^6.1.11",
        "@fullcalendar/list": "^6.1.11",
        "@fullcalendar/multimonth": "^6.1.11",
        "@fullcalendar/timegrid": "^6.1.11",
        "rxjs": "~7.8.1",
        "tslib": "^2.8.0",
        "zone.js": "~0.14.10"
      },
      "devDependencies": {
        "@angular-devkit/build-angular": "^18.2.10",
        "@angular/cli": "^18.2.10",
        "@angular/compiler-cli": "^18.0.0",
            "@types/jasmine": "^5.1.4",
        "jasmine-core": "^5.4.0",
        "karma": "^6.4.4",
        "karma-chrome-launcher": "^3.2.0",
        "karma-coverage": "^2.2.1",
        "karma-jasmine": "^5.1.0",
        "karma-jasmine-html-reporter": "^2.1.0",
        "postcss": "^8.4.47",
        "prettier": "^3.3.3",
        "prettier-eslint": "^16.3.0",
        "prettier-plugin-css-order": "^2.1.2",
        "prettier-plugin-jsdoc": "^1.3.0",
        "prettier-plugin-organize-attributes": "^1.0.0",
        "roboto-fontface": "^0.10.0",
        "source-map-explorer": "^2.5.3",
        "typescript": "^5.5.4"
      }
    }